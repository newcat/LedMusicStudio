{
    "compilerOptions": {
        "target": "ES5",
        "useDefineForClassFields": true,
        "module": "ESNext",
        "moduleResolution": "node",
        "strict": true,
        "jsx": "preserve",
        "sourceMap": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "outDir": "electron-dist"
    },
    "include": ["electron/**/*.ts"]
}
