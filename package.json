{
  "name": "ledmusicstudio",
  "version": "0.0.0",
  "main": "electron/electron.js",
  "license": "GPL-3.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "electron": "yarn build:electron && wait-on tcp:3000 && cross-env IS_DEV=true electron electron-dist/electron.js",
    "electron:pack": "electron-builder --dir",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none yarn dev\" \"yarn electron\"",
    "electron:builder": "electron-builder",
    "build:for:electron": "vue-tsc --noEmit && cross-env ELECTRON=true vite build",
    "app:build": "yarn build:for:electron && yarn electron:builder",
    "build:wasm": "wasm-pack build ./src/rust --target web",
    "build:electron": "tsc -p tsconfig.main.json"
  },
  "dependencies": {
    "@baklavajs/core": "^2.0.2-alpha.7",
    "@baklavajs/engine": "^2.0.2-alpha.7",
    "@baklavajs/events": "^2.0.2-alpha.5",
    "@baklavajs/interface-types": "^2.0.2-alpha.7",
    "@baklavajs/renderer-vue": "^2.0.2-alpha.7",
    "@baklavajs/themes": "^2.0.2-alpha.4",
    "@ckpack/vue-color": "^1.1.8",
    "@vueuse/core": "^7.7.1",
    "axios": "^0.26.1",
    "bson": "^4.6.1",
    "chroma-js": "^2.4.2",
    "comlink": "^4.3.1",
    "open-simplex-noise": "^2.5.0",
    "serialport": "^10.4.0",
    "splitpanes": "^3.1.1",
    "uuid": "^8.3.2",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@types/bson": "^4.2.0",
    "@types/chroma-js": "^2.1.3",
    "@types/offscreencanvas": "^2019.6.4",
    "@types/serialport": "^8.0.2",
    "@types/uuid": "^8.3.4",
    "@vicons/material": "^0.12.0",
    "@vitejs/plugin-vue": "^2.2.4",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "^17.1.2",
    "electron-builder": "^22.14.13",
    "naive-ui": "^2.26.4",
    "postcss": "^8.4.8",
    "sass": "^1.49.9",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.4.4",
    "vfonts": "^0.0.3",
    "vite": "^2.8.6",
    "vue-tsc": "^0.32.1",
    "wait-on": "^6.0.1"
  },
  "build": {
    "appId": "com.github.newcat.ledmusicstudio",
    "productName": "LedMusicStudio",
    "copyright": "Copyright Â© 2022 newcat",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    }
  }
}
