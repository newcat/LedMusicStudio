{
  "name": "ledmusicstudio",
  "version": "0.0.0",
  "main": "electron/electron.js",
  "license": "GPL-3.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "electron": "wait-on tcp:3000 && cross-env IS_DEV=true electron .",
    "electron:pack": "electron-builder --dir",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none yarn dev\" \"yarn electron\"",
    "electron:builder": "electron-builder",
    "build:for:electron": "vue-tsc --noEmit && cross-env ELECTRON=true vite build",
    "app:build": "yarn build:for:electron && yarn electron:builder"
  },
  "dependencies": {
    "@baklavajs/core": "^2.0.1-alpha.0",
    "@baklavajs/engine": "^2.0.1-alpha.0",
    "@baklavajs/events": "^2.0.1-alpha.0",
    "@baklavajs/interface-types": "^2.0.1-alpha.0",
    "@baklavajs/renderer-vue": "^2.0.1-alpha.0",
    "@baklavajs/themes": "^2.0.1-alpha.0",
    "axios": "^0.24.0",
    "bson": "^4.6.1",
    "chroma-js": "^2.1.2",
    "comlink": "^4.3.1",
    "open-simplex-noise": "^2.5.0",
    "serialport": "^9.2.8",
    "splitpanes": "^3.0.6",
    "uuid": "^8.3.2",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@electron/remote": "^2.0.1",
    "@types/bson": "^4.2.0",
    "@types/chroma-js": "^2.1.3",
    "@types/offscreencanvas": "^2019.6.4",
    "@types/serialport": "^8.0.2",
    "@types/uuid": "^8.3.4",
    "@vicons/material": "^0.11.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "^16.0.6",
    "electron-builder": "^22.14.5",
    "naive-ui": "^2.23.2",
    "sass": "^1.47.0",
    "typescript": "^4.4.4",
    "vfonts": "^0.0.3",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8",
    "wait-on": "^6.0.0"
  },
  "build": {
    "appId": "com.github.newcat.ledmusicstudio",
    "productName": "LedMusicStudio",
    "copyright": "Copyright Â© 2022 newcat",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    }
  }
}
